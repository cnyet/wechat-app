/**index.wxss**/
.userinfo {
  display: flex;
  margin: 0 20rpx;
  flex-direction: row;
}

.userinfo-avatar {
  display: inline-block;
  vertical-align: middle;
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #666;
  line-height: 60rpx;
  margin-left: 20rpx;
  font-size: 14px;
}
.content-wrap{
  width: 100%;
  height: 600rpx;
  margin: 100rpx 0;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
  flex-direction: column;
}
.table-row{
  width: 100%;
  height: 104rpx;
  text-align: center;
}
.table-row .table-column{
  display: inline-block;
  vertical-align: middle;
  line-height: 100rpx;
  width: 100rpx;
  height: 100rpx;
  font-size: 14px;
  color: #999;
  background-color: #f8f8f9;
  border: 1px #fff solid;
}
.table-column.color-0{
  background-color: #f8f8f9;
}
.table-column.color-1{
  background-color: #CC0033;
}
.table-column.color-2{
  background-color: #CC6600;
}
.table-column.color-3{
  background-color: #FFCC33;
}
.table-column.color-4{
  background-color: #669900;
}
.table-column.color-5{
  background-color: #0033CC;
}
.table-column.color-6{
  background-color: #33CCCC;
}
.table-column.color-7{
  background-color: #6633FF;
}
.table-column.color-8{
  background-color: #FF99FF;
}
.table-column.color-9{
  background-color: #663333;
}

.content-foot{
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 14px;
  color: #666;
}