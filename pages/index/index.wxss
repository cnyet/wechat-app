/**index.wxss**/
.userinfo {
  display: flex;
  margin: 0 20rpx;
  flex-direction: row;
}

.userinfo-avatar {
  display: inline-block;
  vertical-align: middle;
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #666;
  line-height: 60rpx;
  margin-left: 20rpx;
  font-size: 14px;
}
.content-wrap{  
  height: 312rpx;
  margin: 200rpx 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
  text-align: center;
}
.content-row{
  width: 100%;
  height: 104rpx;
}
.content-row .table-column{
  display: inline-block;
  vertical-align: middle;
  line-height: 100rpx;
  width: 100rpx;
  height: 100rpx;
  font-size: 14px;
  color: #fff;
  background-color: #f8f8f9;
  border: 1rpx #fff solid;
}
.table-column.color-1{
  background-color: #990066;
}
.table-column.color-2{
  background-color: #FFCC00;
}
.table-column.color-3{
  background-color: #CC0033;
}
.table-column.color-4{
  background-color: #FFCC00;
}
.table-column.color-5{
  background-color: #009999;
}
.table-column.color-6{
  background-color: #CC3366;
}
.table-column.color-7{
  background-color: #CC3333;
}
.table-column.color-8{
  background-color: #FFCCCC;
}
.table-column.color-9{
  background-color: #99CC00;
}
.table-column.color-10{
  background-color: #003399;
}
.table-column.color-11{
  background-color: #FF9933;
}
.table-column.color-12{
  background-color: #006600;
}
.content-foot{
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 14px;
  color: #666;
}